<%= simple_form_for(@district) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :permalink %>
    <%= f.input :is_park %>
    <%= f.input :credit %>
    <%= f.input :photo_url %>
    <%= f.input :flickr_search_term %>
    <%= f.input :published_at %>
    <%= f.input :wdw_uri %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
